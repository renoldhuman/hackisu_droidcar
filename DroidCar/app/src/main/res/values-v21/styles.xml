<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@android:color/holo_red_dark</item>
        <item name="colorPrimaryDark">@android:color/primary_text_dark_nodisable</item>
        <item name="colorAccent">@android:color/holo_red_dark</item>
        <item name="android:colorBackground">@android:color/tertiary_text_dark</item>
        <item name="android:colorForeground">@android:color/background_light</item>
        <item name="android:textColorPrimary">@android:color/background_light</item>
    </style>

    <style name="DroidCar" parent="Theme.AppCompat.NoActionBar">
        <item name="android:colorBackground">@android:color/primary_text_light</item>
        <item name="android:textColorPrimary">@android:color/background_light</item>
        <item name="colorAccent">@android:color/holo_red_dark</item>
        <item name="colorPrimary">@android:color/holo_red_dark</item>
    </style>
</resources>